{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/bot.js",
      "use": "@vercel/node"
    },
    {
      "src": "webapp/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/bot/(.*)",
      "dest": "/api/bot.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/webapp/dist/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/webapp/dist/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
